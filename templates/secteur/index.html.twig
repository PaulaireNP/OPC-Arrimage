{% extends 'base.html.twig' %}

{% block title %}Secteur index{% endblock %}

{% block body %}

<div class="main-secteur">

    {% include 'layout/navbarDashboardAdmin.html.twig' %}

    <div class="main-secteur__container-secteur">

    <h1>Secteur index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Mobile</th>
                <th>Mail</th>
                <th>Number</th>
                <th>Street</th>
                <th>AdditionalAddress</th>
                <th>City</th>
                <th>ZipCode</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for secteur in secteurs %}
            <tr>
                <td>{{ secteur.id }}</td>
                <td>{{ secteur.name }}</td>
                <td>{{ secteur.mobile }}</td>
                <td>{{ secteur.mail }}</td>
                <td>{{ secteur.number }}</td>
                <td>{{ secteur.street }}</td>
                <td>{{ secteur.additionalAddress }}</td>
                <td>{{ secteur.city }}</td>
                <td>{{ secteur.zipCode }}</td>
                <td>
                    <a href="{{ path('app_secteur_show', {'id': secteur.id}) }}">show</a>
                    <a href="{{ path('app_secteur_edit', {'id': secteur.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="10">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_secteur_new') }}">Create new</a>

    </div>

</div>
{% endblock %}
