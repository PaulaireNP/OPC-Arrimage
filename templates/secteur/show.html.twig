{% extends 'base.html.twig' %}

{% block title %}Secteur{% endblock %}

{% block body %}
    <h1>Secteur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ secteur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ secteur.name }}</td>
            </tr>
            <tr>
                <th>Mobile</th>
                <td>{{ secteur.mobile }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ secteur.mail }}</td>
            </tr>
            <tr>
                <th>Numéro de rue</th>
                <td>{{ secteur.number }}</td>
            </tr>
            <tr>
                <th>Rue</th>
                <td>{{ secteur.street }}</td>
            </tr>
            <tr>
                <th>Complément d'adresse</th>
                <td>{{ secteur.additionalAddress }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ secteur.city }}</td>
            </tr>
            <tr>
                <th>Code Postal</th>
                <td>{{ secteur.zipCode }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_secteur_index') }}">Quitter sans sauvegarder</a>

    <a href="{{ path('app_secteur_edit', {'id': secteur.id}) }}">Modifier</a>

    {{ include('secteur/_delete_form.html.twig') }}
{% endblock %}
